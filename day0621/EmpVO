//VO for Employee data

@Data
public class EmpVO {
	private int ENO; //pk no
	private String ENAME; //employee name
	private int DNO; //department number
	private int SALARY; //salary
	private int COMM; //incentive bonus
	private Date HIREDATE;
	private String PHONE;
	private String ADDR;
	private int MGR; //manager's eno
	private String JOB; //position
	private String EMAIL;
}
